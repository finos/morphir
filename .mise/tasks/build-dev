#!/usr/bin/env bash
#MISE description="Build the morphir-dev CLI"
#USAGE name build-dev
#USAGE bin build-dev
#USAGE about "Build the morphir-dev CLI"
#USAGE usage "mise run build-dev"

set -euo pipefail

SCRIPT_DIR="$(cd \"$(dirname \"${BASH_SOURCE[0]}\")\" && pwd)"
REPO_ROOT="$(cd \"/../..\" && pwd)"

"/scripts/build-dev.sh" "$@"
