#!/usr/bin/env bash
#MISE description="Check Go formatting"
#USAGE name fmt-check
#USAGE bin fmt-check
#USAGE about "Check Go formatting"
#USAGE usage "mise run fmt-check"

set -euo pipefail

SCRIPT_DIR="$(cd \"$(dirname \"${BASH_SOURCE[0]}\")\" && pwd)"
REPO_ROOT="$(cd \"/../..\" && pwd)"

"/scripts/fmt-check.sh" "$@"
