#!/usr/bin/env bash
#MISE description="Install morphir CLI with go install"
#USAGE name install
#USAGE bin install
#USAGE about "Install morphir CLI with go install"
#USAGE usage "mise run install"

set -euo pipefail

SCRIPT_DIR="$(cd \"$(dirname \"${BASH_SOURCE[0]}\")\" && pwd)"
REPO_ROOT="$(cd \"/../..\" && pwd)"

cd ""
go install ./cmd/morphir
