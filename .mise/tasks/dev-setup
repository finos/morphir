#!/usr/bin/env bash
#MISE description="Set up local Go workspace"
#USAGE name dev-setup
#USAGE bin dev-setup
#USAGE about "Set up local Go workspace"
#USAGE usage "mise run dev-setup"

set -euo pipefail

SCRIPT_DIR="$(cd \"$(dirname \"${BASH_SOURCE[0]}\")\" && pwd)"
REPO_ROOT="$(cd \"/../..\" && pwd)"

"/scripts/dev-setup.sh" "$@"
