#!/usr/bin/env bash
#MISE description="Clean build artifacts"
#USAGE name clean
#USAGE bin clean
#USAGE about "Clean build artifacts"
#USAGE usage "mise run clean"

set -euo pipefail

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
REPO_ROOT="$(cd "$SCRIPT_DIR/../.." && pwd)"

"$REPO_ROOT/scripts/clean.sh" "$@"
