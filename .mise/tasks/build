#!/usr/bin/env bash
#MISE description="Build the morphir CLI"
#USAGE name build
#USAGE bin build
#USAGE about "Build the morphir CLI"
#USAGE usage "mise run build"

set -euo pipefail

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
REPO_ROOT="$(cd "$SCRIPT_DIR/../.." && pwd)"

"$REPO_ROOT/scripts/build.sh" "$@"
