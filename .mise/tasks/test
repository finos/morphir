#!/usr/bin/env bash
#MISE description="Run all Go tests"
#USAGE name test
#USAGE bin test
#USAGE about "Run all Go tests"
#USAGE usage "mise run test"

set -euo pipefail

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
REPO_ROOT="$(cd "$SCRIPT_DIR/../.." && pwd)"

"$REPO_ROOT/scripts/test.sh" "$@"
