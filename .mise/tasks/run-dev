#!/usr/bin/env bash
#MISE description="Run morphir-dev CLI"
#USAGE name run-dev
#USAGE bin run-dev
#USAGE about "Run morphir-dev CLI"
#USAGE usage "mise run run-dev"

set -euo pipefail

SCRIPT_DIR="$(cd \"$(dirname \"${BASH_SOURCE[0]}\")\" && pwd)"
REPO_ROOT="$(cd \"/../..\" && pwd)"

"/scripts/run-dev.sh" "$@"
