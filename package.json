{
  "name": "morphir-home",
  "private": true,
  "workspaces": ["apps/morphir-cli", "packages/*", "tools/*"],
  "scripts": {
    "bld": "dotnet run --project build/build.fsproj",
    "build": "moon run :build -u",
    "clean": "moon run clean",
    "elm-build": "moon run :elm-build",
    "purge": "moon run purge",
    "check-elm-docs": "moon run check-elm-docs",
    "list-projects": "moon project-graph --json | npx node-jq '.projects| keys[]'",
    "morphirjs": "moon run js-cli:run"
  },
  "devDependencies": {
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.7",
    "elm-test-rs": "^3.0.0-5"
  }
}
